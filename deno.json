{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "esm:add": "deno run -A https://esm.sh/v132 add",
    "esm:update": "deno run -A https://esm.sh/v132 update",
    "esm:remove": "deno run -A https://esm.sh/v132 remove",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.5/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.1",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "@twind/preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4/",
    "@twind/preset-tailwind/": "https://esm.sh/v132/*@twind/preset-tailwind@1.1.4/",
    "immer": "https://esm.sh/v132/immer@9.0.21",
    "immer/": "https://esm.sh/v132/immer@9.0.21/",
    "preact-render-to-string": "https://esm.sh/v132/*preact-render-to-string@5.2.6",
    "preact-render-to-string/": "https://esm.sh/v132/*preact-render-to-string@5.2.6/",
    "preact-transitioning": "https://esm.sh/v132/*preact-transitioning@1.4.0",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "twind": "https://esm.sh/v132/*twind@0.16.19",
    "twind/": "https://esm.sh/v132/*twind@0.16.19/",
    "@twind/preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7/"
  },
  "scopes": {
    "https://esm.sh/v115/": {
      "csstype": "https://esm.sh/v115/csstype@3.1.2",
      "preact-merge-refs": "https://esm.sh/v115/preact-merge-refs@1.0.2"
    },
    "https://esm.sh/v122/": {
      "csstype": "https://esm.sh/v122/csstype@3.1.2",
      "htmlparser2": "https://esm.sh/v122/htmlparser2@6.1.0",
      "preact-merge-refs": "https://esm.sh/v122/preact-merge-refs@1.0.2",
      "pretty-format": "https://esm.sh/v122/pretty-format@3.8.0",
      "style-vendorizer": "https://esm.sh/v122/style-vendorizer@2.2.3"
    },
    "https://esm.sh/v132/": {
      "csstype": "https://esm.sh/v132/csstype@3.1.2",
      "htmlparser2": "https://esm.sh/v132/htmlparser2@6.1.0",
      "preact-merge-refs": "https://esm.sh/v132/preact-merge-refs@1.0.2",
      "pretty-format": "https://esm.sh/v132/pretty-format@3.8.0",
      "style-vendorizer": "https://esm.sh/v132/style-vendorizer@2.2.3"
    }
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"]
}
